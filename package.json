{
  "name": "gnss-compute-server",
  "version": "1.0.0",
  "description": "High-performance GNSS computation server with Pull Consumer architecture",
  "main": "dist/external-worker.js",
  "type": "module",
  "scripts": {
    "start": "npm run build && node dist/external-worker.js",
    "dev": "npm run build && node --watch dist/external-worker.js",
    "dev:ts": "tsx watch external-worker.ts",
    "build": "tsc",
    "build:watch": "tsc --watch"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/node": "^20.0.0",
    "tsx": "^4.20.4",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "gnss",
    "computation",
    "pull-consumer",
    "typescript",
    "cloudflare-workers"
  ]
}
